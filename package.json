    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,md,json}": [
            "prettier --write"
        ]
    },
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "eject": "expo eject",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "test:watch": "npm test -- --watch",
        "test": "jest --watch --coverage=false --changedSince=origin/master",
        "testDebug": "jest -o --watch --coverage=false",
        "testFinal": "jest",
        "updateSnapshots": "jest -u --coverage=false",
        "styleCheck": "prettier --check 'src/**/*.js'",
        "build": "tsc"
    },
    "jest": {
        "preset": "jest-expo",
        "transformIgnorePatterns": [
            "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|sentry-expo|native-base|@sentry/react-native)"
        ],
        "moduleNameMapper": {
            ".+\\.(css|styl|less|sass|scss|ttf|woff|woff2)$": "identity-obj-proxy",
            "^[./a-zA-Z0-9$_-]+\\.png$": "<rootDir>/__tests__/__mocks__/RelativeImageStub.js"
        },
        "setupFiles": [
            "./__tests__/config/globalFetch.js",
            "./__tests__/config/setup.js"
        ],
        "testEnvironment": "jsdom",
        "testPathIgnorePatterns": [
            "/__mocks__/",
            "/node_modules/",
            "/config/"
        ]
    },
